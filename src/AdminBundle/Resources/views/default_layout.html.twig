<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <title>latte cake - admin home</title>

    {% set admin_static = 'http://demo.lattecake.com/Template/ACE/' %}

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />

    <!--[if !IE]> -->
    <link rel="stylesheet" href="{{ admin_static }}assets/css/pace.css" />

    <script data-pace-options='{ "ajax": true, "document": true, "eventLag": false, "elements": false }' src="{{ admin_static }}assets/js/pace.js"></script>

    <!-- <![endif]-->

    <!-- bootstrap & fontawesome -->
    <link rel="stylesheet" href="{{ admin_static }}assets/css/bootstrap.css" />
    <link rel="stylesheet" href="{{ admin_static }}assets/css/font-awesome.css" />

    <!-- text fonts -->
    <link rel="stylesheet" href="{{ admin_static }}assets/css/ace-fonts.css" />

    <!-- ace styles -->
    <link rel="stylesheet" href="{{ admin_static }}assets/css/ace.css" class="ace-main-stylesheet" id="main-ace-style" />

    <!--[if lte IE 9]>
    <link rel="stylesheet" href="{{ admin_static }}assets/css/ace-part2.css" class="ace-main-stylesheet" />
    <![endif]-->

    <!--[if lte IE 9]>
    <link rel="stylesheet" href="{{ admin_static }}assets/css/ace-ie.css" />
    <![endif]-->

    <!-- ace settings handler -->
    <script src="{{ admin_static }}assets/js/ace-extra.js"></script>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->

    <!--[if lte IE 8]>
    <script src="{{ admin_static }}assets/js/html5shiv.js"></script>
    <script src="{{ admin_static }}assets/js/respond.js"></script>
    <![endif]-->
    <script>
        base_url = '{{ url('admin_index') }}';
    </script>
</head>

<body class="no-skin">

{% include 'AdminBundle::default_navbar.html.twig' %}

<!-- /section:basics/navbar.layout -->
<div class="main-container" id="main-container">
    <script type="text/javascript">
        try{ace.settings.check('main-container' , 'fixed')}catch(e){}
    </script>

    <!-- #section:basics/sidebar -->
    {% include "AdminBundle::default_sidebar.html.twig" %}

    <!-- /section:basics/sidebar -->
    <div class="main-content">
        <div class="main-content-inner">
            {% block main_content %}{% endblock %}
        </div>
    </div><!-- /.main-content -->

    {% include 'AdminBundle::default_footer.html.twig' %}

    <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
        <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
    </a>
</div><!-- /.main-container -->

<!-- basic scripts -->

<!--[if !IE]> -->
<script type="text/javascript">
    window.jQuery || document.write("<script src='{{ admin_static }}assets/js/jquery.js'>"+"<"+"/script>");
</script>

<!-- <![endif]-->

<!--[if IE]>
<script type="text/javascript">
