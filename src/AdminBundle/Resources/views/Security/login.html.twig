<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <title>登陆页 - LatteCake管理后台</title>

    <meta name="description" content="User login page" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />

    <!-- bootstrap & fontawesome -->
    <link rel="stylesheet" href="http://demo.lattecake.com/Template/ACE/assets/css/bootstrap.css" />
    <link rel="stylesheet" href="http://demo.lattecake.com/Template/ACE/assets/css/font-awesome.css" />

    <!-- text fonts -->
    <link rel="stylesheet" href="http://demo.lattecake.com/Template/ACE/assets/css/ace-fonts.css" />

    <!-- ace styles -->
    <link rel="stylesheet" href="http://demo.lattecake.com/Template/ACE/assets/css/ace.css" />

    <!--[if lte IE 9]>
    <link rel="stylesheet" href="http://demo.lattecake.com/Template/ACE/assets/css/ace-part2.css" />
    <![endif]-->
    <link rel="stylesheet" href="http://demo.lattecake.com/Template/ACE/assets/css/ace-rtl.css" />

    <!--[if lte IE 9]>
    <link rel="stylesheet" href="http://demo.lattecake.com/Template/ACE/assets/css/ace-ie.css" />
    <![endif]-->

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->

    <!--[if lt IE 9]>
    <script src="http://demo.lattecake.com/Template/ACE/assets/js/html5shiv.js"></script>
    <script src="http://demo.lattecake.com/Template/ACE/assets/js/respond.js"></script>
    <![endif]-->
</head>

<body class="login-layout blur-login">
<div class="main-container">
    <div class="main-content">
        <div class="row">
            <div class="col-sm-10 col-sm-offset-1">
                <div class="login-container">
                    <div class="center">
                        <h1>
                            <i class="ace-icon fa fa-leaf green"></i>
                            <span class="red">LatteCake</span>
                            <span class="white" id="id-text2">后台管理</span>
                        </h1>
                        <h4 class="light-blue" id="id-company-text">&copy; superCong</h4>
                    </div>

                    <div class="space-6"></div>

                    <div class="position-relative">
                        <div id="login-box" class="login-box visible widget-box no-border">
                            <div class="widget-body">
                                <div class="widget-main">
                                    {% trans_default_domain 'FOSUserBundle' %}
                                    <h4 class="header blue lighter bigger">
                                        <i class="ace-icon fa fa-coffee green"></i>
                                        请输入您的信息
                                    </h4>

                                    {% if error %}
                                        {{ error.messageKey|trans(error.messageData, 'security') }}
                                    {% endif %}

                                    <div class="space-6"></div>

                                    <form action="{{ url('fos_user_security_check') }}" data-ajax="false" method="post">
                                        <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
                                        <fieldset>
                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">
                                                    <input type="text" class="form-control" id="username" name="_username" value="{{ last_username }}" required="required" placeholder="{{ 'security.login.username'|trans }}" />
                                                    <i class="ace-icon fa fa-user"></i>
                                                </span>
                                            </label>

                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">
                                                    <input type="password" id="password" name="_password" required="required" class="form-control" placeholder="{{ 'security.login.password'|trans }}" />
                                                    <i class="ace-icon fa fa-lock"></i>
                                                </span>
                                            </label>

                                            <div class="space"></div>

                                            <div class="clearfix">
                                                <label class="inline">
                                                    <input type="checkbox" id="remember_me" name="_remember_me" value="on" class="ace" />
                                                    <span class="lbl"> {{ 'security.login.remember_me'|trans }}</span>
                                                </label>
                                                <input type="hidden" name="_target_path" value="{{ url('admin_index') }}" />
                                                <button type="submit" id="_submit" name="_submit" class="width-35 pull-right btn btn-sm btn-primary">
                                                    <i class="ace-icon fa fa-key"></i>
                                                    <span class="bigger-110">{{ 'security.login.submit'|trans }}</span>
                                                </button>
                                            </div>

                                            <div class="space-4"></div>
                                        </fieldset>
                                    </form>

                                    {#<div class="social-or-login center">#}
                                        {#<span class="bigger-110">或其他方式登陆</span>#}
                                    {#</div>#}

                                    {#<div class="space-6"></div>#}

                                    {#<div class="social-login center">#}
                                        {#<a class="btn btn-primary">#}
                                            {#<i class="ace-icon fa fa-facebook"></i>#}
                                        {#</a>#}

                                        {#<a class="btn btn-info">#}
                                            {#<i class="ace-icon fa fa-twitter"></i>#}
                                        {#</a>#}

                                        {#<a class="btn btn-danger">#}
                                            {#<i class="ace-icon fa fa-google-plus"></i>#}
                                        {#</a>#}
                                    {#</div>#}
                                </div><!-- /.widget-main -->

                                {#<div class="toolbar clearfix">#}
                                    {#<div>#}
                                        {#<a href="#" data-target="#forgot-box" class="forgot-password-link">#}
                                            {#<i class="ace-icon fa fa-arrow-left"></i>#}
                                            {#找回密码#}
                                        {#</a>#}
                                    {#</div>#}

                                    {#<div>#}
                                        {#<a href="#" data-target="#signup-box" class="user-signup-link">#}
                                            {#我要注册#}
                                            {#<i class="ace-icon fa fa-arrow-right"></i>#}
                                        {#</a>#}
                                    {#</div>#}
                                {#</div>#}
                            </div><!-- /.widget-body -->
                        </div><!-- /.login-box -->
                    </div><!-- /.position-relative -->
                </div>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.main-content -->
</div><!-- /.main-container -->

<!-- basic scripts -->

<!--[if !IE]> -->
<script type="text/javascript">
    window.jQuery || document.write("<script src='http://demo.lattecake.com/Template/ACE/assets/js/jquery.js'>"+"<"+"/script>");
</script>

<!-- <![endif]-->

<!--[if IE]>
<script type="text/javascript">
    window.jQuery || document.write("<script src='http://demo.lattecake.com/Template/ACE/assets/js/jquery1x.js'>"+"<"+"/script>");
</script>
<![endif]-->
<script type="text/javascript">
    if('ontouchstart' in document.documentElement) document.write("<script src='http://demo.lattecake.com/Template/ACE/assets/js/jquery.mobile.custom.js'>"+"<"+"/script>");
</script>

</body>
</html>
